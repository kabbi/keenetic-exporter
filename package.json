{
  "name": "keenetic-adapter",
  "display_name": "keenetic adapter",
  "version": "0.1.0",
  "description": "Brings all your wifi devices to WoT Gateway",
  "main": "index.js",
  "keywords": [
    "mozilla",
    "iot",
    "adapter",
    "virtual"
  ],
  "homepage": "https://github.com/kabbi/keenetic-adapter",
  "author": "Mozilla IoT",
  "license": "MPL-2.0",
  "repository": {
    "type": "git",
    "url": "https://github.com/kabbi/keenetic-adapter.git"
  },
  "bugs": {
    "url": "https://github.com/kabbi/keenetic-adapter/issues"
  },
  "dependencies": {
    "crypto-js": "^3.1.9-1",
    "fast-xml-parser": "^3.13.0",
    "lodash": "^4.17.21",
    "node-fetch": "^2.6.0"
  },
  "devDependencies": {
    "eslint": "^4.19.1",
    "sqlite3": "^4.1.0"
  },
  "files": [
    "LICENSE",
    "SHA256SUMS",
    "index.js",
    "keenetic-adapter.js",
    "digest-fetch.js"
  ],
  "moziot": {
    "api": {
      "min": 1,
      "max": 2
    },
    "plugin": true,
    "exec": "{nodeLoader} {path}",
    "config": {
      "url": "http://192.168.1.1/ci",
      "username": "username",
      "password": "password"
    },
    "schema": {
      "type": "object",
      "properties": {
        "url": {
          "type": "string",
          "description": "Root url of your router"
        },
        "username": {
          "type": "string",
          "description": "Username"
        },
        "password": {
          "type": "string",
          "description": "Password"
        }
      }
    }
  }
}
